{
  "name": "master-class",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref uitwvdydxvdjkpjlctsy",
    "db:migrate:new": "supabase migration new",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env ./database/seed.js",
    "supabase:types": "npx supabase gen types typescript --project-id uitwvdydxvdjkpjlctsy --schema public > database/types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/vue-table": "^8.20.4",
    "@vueuse/core": "^11.1.0",
